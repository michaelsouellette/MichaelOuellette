{
	"html.format.wrapAttributes": "force",
	"html.format.endWithNewline": true,
	"stylelint.enable": true,
	"stylelint.autoFixOnSave": true,
	"editor.insertSpaces": false,
	"editor.tabSize": 4,
	"editor.detectIndentation": false,
	"appService.zipIgnorePattern": [
		"node_modules{,/**}",
		".vscode{,/**}"
	]
}
