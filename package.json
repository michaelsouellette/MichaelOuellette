    "@angular/language-service": "~14.1.0",
    "@nguniversal/builders": "^13.0.2",
    "@types/express": "^4.17.11",
    "@types/jasmine": "~4.0.0",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^14.14.37",
