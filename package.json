{
	"name": "app",
	"version": "1.1.0",
	"scripts": {
		"ng": "ng",
		"start": "npm run serve:ssr",
		"deploy": "ng build --prod && npm run build:ssr && gcloud app deploy",
		"build": "ng build",
		"build:production": "npm run build:ssr && npm run build:copy:files",
		"build:copy:files": "cp app.yaml dist/app.yaml && cp package.json dist/package.json && cp .gcloudignore dist/.gcloudignore",
		"test": "ng test",
		"lint": "ng lint",
		"e2e": "ng e2e",
		"dev:ssr": "ng run app:serve-ssr",
		"serve:ssr": "node app/server/main.js",
		"build:ssr": "ng build --prod && ng run app:server:production",
		"prerender": "ng run app:prerender"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "~11.0.0",
		"@angular/common": "~11.0.0",
		"@angular/compiler": "~11.0.0",
		"@angular/core": "~11.0.0",
		"@angular/forms": "~11.0.0",
		"@angular/platform-browser": "~11.0.0",
		"@angular/platform-browser-dynamic": "~11.0.0",
		"@angular/platform-server": "~11.2.12",
		"@angular/router": "~11.0.0",
		"@mdi/font": "^6.1.95",
		"@nguniversal/express-engine": "^11.0.0",
		"express": "^4.15.2",
		"rxjs": "~6.6.7",
		"tslib": "^2.3.0",
		"tslint-angular": "^3.0.3",
		"zone.js": "~0.10.2"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "~0.1102.11",
		"@angular/cli": "~12.1.4",
		"@angular/compiler-cli": "~11.0.0",
		"@angular/language-service": "~12.2.1",
		"@nguniversal/builders": "^11.2.1",
		"@types/express": "^4.17.13",
		"@types/jasmine": "~3.8.2",
		"@types/jasminewd2": "~2.0.10",
		"@types/node": "^15.6.2",
		"codelyzer": "^6.0.2",
		"jasmine-core": "~3.7.1",
		"jasmine-spec-reporter": "~7.0.0",
		"karma": "~6.3.4",
