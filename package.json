{
  "name": "app",
  "version": "1.1.0",
  "scripts": {
    "ng": "ng",
    "start": "npm run serve:ssr",
    "deploy": "ng build --configuration production && npm run build:ssr && gcloud app deploy",
    "build": "ng build",
    "build:production": "npm run build:ssr && npm run build:copy:files",
    "build:copy:files": "cp app.yaml dist/app.yaml && cp package.json dist/package.json && cp .gcloudignore dist/.gcloudignore",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run app:serve-ssr",
    "serve:ssr": "node app/server/main.js",
    "build:ssr": "ng build --configuration production && ng run app:server:production",
    "prerender": "ng run app:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.14",
    "@angular/common": "~12.2.14",
    "@angular/compiler": "~12.2.14",
    "@angular/core": "~12.2.14",
    "@angular/forms": "~12.2.14",
    "@angular/platform-browser": "~12.2.14",
    "@angular/platform-browser-dynamic": "~12.2.14",
    "@angular/platform-server": "~12.2.14",
    "@angular/router": "~12.2.14",
    "@mdi/font": "^5.9.55",
    "@nguniversal/express-engine": "^12.1.3",
    "angular-cli-ghpages": "^1.0.0",
    "express": "^4.15.2",
    "rxjs": "~6.6.7",
    "tslib": "^2.1.0",
    "tslint-angular": "^3.0.3",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.14",
    "@angular/cli": "~12.2.14",
    "@angular/compiler-cli": "~12.2.14",
    "@angular/language-service": "~12.2.14",
    "@nguniversal/builders": "^12.1.3",
    "@types/express": "^4.17.11",
    "@types/jasmine": "~3.6.9",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^16.11.12",
    "angular-prerender": "^10.0.1",
    "codelyzer": "^6.0.2",
    "jasmine-core": "~3.7.1",
    "jasmine-spec-reporter": "~6.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "rxjs-tslint-rules": "^4.34.8",
    "ts-node": "~9.1.1",
    "tslint": "~6.1.3",
